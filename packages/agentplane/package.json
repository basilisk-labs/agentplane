{
  "name": "agentplane",
  "version": "0.2.26",
  "description": "Agent Plane CLI for task workflows, recipes, and project automation.",
  "keywords": [
    "agentplane",
    "agentplane-framework",
    "cli",
    "tasks",
    "workflow",
    "recipes",
    "automation",
    "orchestration"
  ],
  "license": "MIT",
  "author": "basilisk-labs",
  "maintainers": [
    "basilisk-labs"
  ],
  "contributors": [
    "basilisk-labs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basilisk-labs/agentplane.git"
  },
  "homepage": "https://github.com/basilisk-labs/agentplane",
  "bugs": {
    "url": "https://github.com/basilisk-labs/agentplane/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "agentplane": "bin/agentplane.js"
  },
  "files": [
    "bin",
    "dist",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "clean": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true}); require('node:fs').rmSync('tsconfig.tsbuildinfo',{force:true});\"",
    "build": "tsc -b",
    "typecheck": "tsc -b",
    "prepare": "npm run build",
    "prepack": "npm run clean && npm run build",
    "prepublishOnly": "node ../../scripts/enforce-github-publish.mjs && npm run prepack"
  },
  "dependencies": {
    "@agentplaneorg/core": "0.2.26",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/yauzl": "^2.10.0"
  }
}
