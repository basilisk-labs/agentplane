AGENTPLANE_REDMINE_URL=https://redmine.example.com
AGENTPLANE_REDMINE_API_KEY=replace-me
AGENTPLANE_REDMINE_PROJECT_ID=example-project-key
AGENTPLANE_REDMINE_ASSIGNEE_ID=123
# Optional owner label used by Redmine sync
AGENTPLANE_REDMINE_OWNER=CODER
# Optional alias for owner label (if provided, used like AGENTPLANE_REDMINE_OWNER)
AGENTPLANE_REDMINE_OWNER_AGENT=CODER
# Custom field IDs from Redmine admin (Administration -> Custom fields -> Issue)
AGENTPLANE_REDMINE_CUSTOM_FIELDS_TASK_ID=1
AGENTPLANE_REDMINE_CUSTOM_FIELDS_DOC=2
AGENTPLANE_REDMINE_CUSTOM_FIELDS_DOC_VERSION=3
AGENTPLANE_REDMINE_CUSTOM_FIELDS_DOC_UPDATED_AT=4
AGENTPLANE_REDMINE_CUSTOM_FIELDS_DOC_UPDATED_BY=5
AGENTPLANE_REDMINE_CUSTOM_FIELDS_TAGS=6
AGENTPLANE_REDMINE_CUSTOM_FIELDS_PRIORITY=7
AGENTPLANE_REDMINE_CUSTOM_FIELDS_OWNER=8
# Batch tuning (optional)
AGENTPLANE_REDMINE_BATCH_SIZE=50
AGENTPLANE_REDMINE_BATCH_PAUSE=200
